<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Auditores</servlet-name>
        <servlet-class>Servelets.Auditores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Auditor_Roles</servlet-name>
        <servlet-class>Servelets.Auditor_Roles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Verificacion</servlet-name>
        <servlet-class>Servelets.Verificacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificarCodigo</servlet-name>
        <servlet-class>Servelets.VerificarCodigo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Auditores</servlet-name>
        <url-pattern>/Auditores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Auditor_Roles</servlet-name>
        <url-pattern>/Auditor_Roles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Verificacion</servlet-name>
        <url-pattern>/Verificacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificarCodigo</servlet-name>
        <url-pattern>/VerificarCodigo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
